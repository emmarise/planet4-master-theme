{% block ml_list_content %}
    <div class="ml-media-frame-content" style="margin: 10px 10px 10px 10px;">
        <div class="media-toolbar-primary search-form">
            <label for="ml-media-search-input" class="screen-reader-text">Search Media</label>
            <input type="search" placeholder="Search media items..." id="ml-media-search-input" class="ml-search">
        </div>
        <br>
        <div class="ml-media-list">
            {% for image in data.image_list %}
                <a href="#" id="imageid-{{ image['gpml_image_id'] }}" class="image-thumbnail">
                    <img src="{{ image['image_url'] }}"  width="150px" height="150px">
                </a>
            {% endfor %}
        </div>

        <button class="btn btn-medium btn-small btn-secondary more-btn  btn-load-more-click-scroll" data-current_page="1">SHOW 5 MORE RESULTS</button>
    </div>
    <div class="ml-media-frame-toolbar">
        <div class="ml-media-toolbar">
            <div class="ml-media-toolbar-secondary">
                <div class="ml-media-selection empty">
                    <div class="selection-info">
                        <span class="count">0 selected</span>
                        <button type="button" class="button-link edit-selection">Edit Selection</button>
                        <button type="button" class="button-link clear-selection">Clear</button>
                    </div>
                    <div class="selection-view">
                        <ul tabindex="-1" class="attachments" id="__attachments-view-xxx"></ul>
                    </div>
                </div>
            </div>
            <div class="ml-media-toolbar-primary search-form">
                <button type="button" class="button media-button button-primary button-large media-button-insert" disabled="disabled">Insert into WP media & post</button>
            </div>
        </div>
    </div>
{% endblock %}
